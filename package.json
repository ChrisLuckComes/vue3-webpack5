    "@typescript-eslint/parser": "^5.59.11",
    "@vue/compiler-sfc": "^3.3.4",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-plugin-import": "^1.13.6",
    "copy-webpack-plugin": "^11.0.0",
